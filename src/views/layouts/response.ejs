    <div class="container m-3">
      <% for (const key in dataobj) { %>
        <p class="mb-2"><span class="font-semibold">
            <%= `${key}:` %>
          </span> <span>
            <%= getValue(dataobj[key]) %>
          </span></p>
        <% } %>
    </div>

    <% function getValue(value) { if (Array.isArray(value)) { return value.map(item=> JSON.stringify(item)).join(', ');
      } else if (typeof value === 'object') {
      return JSON.stringify(value);
      } else {
      return value;
      }
      } %>
